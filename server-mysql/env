GRAFANA_API_SERVER_MYSQL_PORT="3001"
GRAFANA_API_SERVER_MYSQL_Q1_GET_REQ_URL="/api/grafana-api-server-mysql/q1-get"
GRAFANA_API_SERVER_MYSQL_Q1_GET_SQL_QUERY="( SELECT USERNAME, EMAIL, DESCRIPTION, TYPE FROM FEEDBACKS WHERE USERNAME = ? ORDER BY CREATED DESC LIMIT 1 ) UNION ( SELECT '${__user.login}' AS USERNAME, '${__user.email}' AS EMAIL, 'Fill in description' AS DESCRIPTION, 'Select type' AS TYPE ) LIMIT 1"
GRAFANA_API_SERVER_MYSQL_Q1_POST_REQ_URL="/api/grafana-api-server-mysql/q1-post"
GRAFANA_API_SERVER_MYSQL_Q1_POST_SQL_QUERY="INSERT INTO FEEDBACKS(CREATED, USERNAME, EMAIL, DESCRIPTION, TYPE) VALUES(NOW(), ?, ?, ?, ?)"
GRAFANA_API_SERVER_MYSQL_Q1_POST_SQL_QUERY_VALUES="USERNAME, EMAIL, DESCRIPTION, TYPE"
GRAFANA_API_SERVER_MYSQL_HOST="<mysql_db_host>:<port>"
GRAFANA_API_SERVER_MYSQL_USER="<mysql_db_user>"
GRAFANA_API_SERVER_MYSQL_PASSWORD="<mysql_db_password>"
GRAFANA_API_SERVER_MYSQL_DB="<mysql_db_db>"
WAIT_HOSTS="<mysql_db_host>:<port>"
WAIT_SLEEP_INTERVAL="30"
WAIT_HOST_CONNECT_TIMEOUT="30"
